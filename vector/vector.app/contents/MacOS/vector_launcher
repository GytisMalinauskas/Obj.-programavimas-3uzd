#!/bin/bash
echo "Select optimization level:"
echo "1) O1"
echo "2) O2"
echo "3) O3"
read -p "Enter choice [1-3]: " choice
case $choice in
  1) exec "$(dirname "$0")/vector_O1" ;;
  2) exec "$(dirname "$0")/vector_O2" ;;
  3) exec "$(dirname "$0")/vector_O3" ;;
  *) echo "Invalid choice"; exit 1 ;;
esac
